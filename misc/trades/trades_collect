/var/log/trades_collect.log {
    daily
    rotate 30
    dateext
    compress
    missingok
    notifempty
    create 0640 root adm
    maxage 30
}

/var/log/trades_collect.error.log {
    daily
    rotate 30
    dateext
    compress
    missingok
    notifempty
    create 0640 root adm
    maxage 30
}
